{
  "name": "boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate for Web Projects",
  "scripts": {
    "serve": "browser-sync start --server 'src' --files 'src'",
    "stylus": "stylus -u nib -u rupture -u s-grid -u typographic -w src/stylus/app.styl -o src/css",
    "foundation": "sass --watch src/foundation:src/css",
    "bootstrap":"sass --watch src/bootstrap:src/css",
    "deploy": "gulp && gulp deploy",
    "start": "npm run serve & npm run stylus",
    "start-foundation": "npm run serve & npm run foundation",
    "start-bootstrap":"npm run serve & npm run bootstrap"
  },
  "author": "SÃ©bastien Pereda",
  "license": "MIT",
  "devDependencies": {
    "bootstrap": "^4.1.1",
    "foundation-sites": "^6.4.4-rc1",
    "gulp": "^3.9.1",
    "gulp-clean-css": "^3.9.4",
    "gulp-gh-pages": "^0.5.4",
    "gulp-if": "^2.0.2",
    "gulp-imagemin": "^4.1.0",
    "gulp-sass": "^4.0.1",
    "gulp-stylus": "^2.7.0",
    "gulp-uglify": "^3.0.0",
    "gulp-useref": "^3.1.5",
    "nib": "^1.1.2",
    "rupture": "^0.7.1",
    "s-grid": "^1.2.1",
    "typographic": "^3.0.0"
  },
  "dependencies": {}
}
